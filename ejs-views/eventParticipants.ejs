<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
<body>
  <h1>"Awesome event" participants</h1>
  <div class="container">
    <% if (participants.length) { %>
      <% participants.forEach(({ fullName, email}) => { %>
        <div class="participant">
          <div><%= fullName %></div>
          <div><%= email %></div>
        </div>
      <% }); %>
    <% } %>
    
  </div>
</body>
</html>